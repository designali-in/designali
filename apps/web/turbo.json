{
  "$schema": "https://turborepo.org/schema.json",
  "extends": ["//"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**", "next-env.d.ts"],
      "env": ["GOOGLE_API_KEY", "CRON_SECRET"]
    },
    "dev": {
      "persistent": true
    }
  }
}
