{
  "name": "dicons",
  "private": false,
  "description": "A react icons library for Designali",
  "version": "0.4.0",
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf .turbo dist node_modules",
    "format": "prettier --write --check . --ignore-path ./.gitignore",
    "lint": "eslint",
    "lint:fix": "eslint --fix .",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "keywords": [
    "designali",
    "shapes",
    "abstract",
    "svg",
    "vector",
    "avatar",
    "illustration",
    "graphics",
    "placeholder"
  ],
  "author": {
    "name": "Designali",
    "email": "contact@designali.in",
    "twitter": "https://x.com/designali_in",
    "url": "https://designali.in"
  },
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./index.ts",
    "./*": [
      "./src/*.tsx",
      "./src/*.ts"
    ]
  },
  "license": "MIT",
  "devDependencies": {
    "@designali/eslint-config": "workspace:*",
    "tsup": "^8.2.4",
    "typescript": "^5.4.5"
  },
  "prettier": "@designali/prettier-config"
}
